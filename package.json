  },
  "engines": {
    "node": ">=22"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/module.js",
        "maxSize": "400KB"
      }
    ]
  },
  "packageManager": "yarn@4.3.1"
}
