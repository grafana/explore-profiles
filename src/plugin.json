{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Profiles",
  "preload": false,
  "id": "grafana-pyroscope-app",
  "backend": true,
  "executable": "gpx_pyroscope_app",
  "dependencies": {
    "grafanaDependency": ">=10.4.3",
    "plugins": []
  },
  "info": {
    "keywords": ["app"],
    "description": "Continuous profiling service powered by Grafana Pyroscope",
    "author": {
      "name": "Grafana"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Tag explorer (LEGACY)",
      "path": "/a/%PLUGIN_ID%/tag-explorer",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Explore profiles",
      "path": "/a/%PLUGIN_ID%/profiles-explorer",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Single view",
      "path": "/a/%PLUGIN_ID%/single",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Comparison view",
      "path": "/a/%PLUGIN_ID%/comparison",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Comparison diff view",
      "path": "/a/%PLUGIN_ID%/comparison-diff",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Ad hoc view",
      "path": "/a/%PLUGIN_ID%/ad-hoc",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": false
    },
    {
      "type": "page",
      "name": "Settings",
      "path": "/a/%PLUGIN_ID%/settings",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": false
    }
  ],
  "extensions": [
    {
      "extensionPointId": "plugins/grafana-pyroscope-datasource/query-links",
      "title": "Profiles App",
      "description": "Open query in Profiles App",
      "type": "link"
    }
  ]
}
