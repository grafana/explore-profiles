{
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["@grafana/runtime", "@grafana/e2e", "@grafana/e2e-selectors/*"]
      }
    ]
  },
  "overrides": [
    {
      "files": ["**/*.{test,story}.{ts,tsx}"],
      "rules": {
        "no-restricted-imports": "off",
        "react/prop-types": "off"
      }
    }
  ]
}
